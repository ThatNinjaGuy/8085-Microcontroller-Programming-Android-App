<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="vishalgautam.a8085emulator">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher"
        android:screenOrientation="portrait"
        android:supportsRtl="true"
        android:theme="@style/MyMaterialTheme.Base">
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Interface"
            android:label="@string/app_name" />
        <activity
            android:name=".Emulator"
            android:label="@string/app_name"
            android:windowSoftInputMode="stateVisible" />
        <activity android:name=".Instructions" />
        <activity android:name=".Tutorials" />
        <activity android:name=".SampleCodes" />
        <activity android:name=".Codes"></activity>
    </application>

</manifest>